pipeline {
agent any
  stages {
    stage ('BUILD') {
      
      steps {
        echo "This is build"
      }
    }
    stage ('TEST') {
      
      steps {
        echo "This is test"
      }
    }
    stage ('DEPLOY') {
      
      steps {
        echo "This is deploy"
      }
    }
    
  }
}
